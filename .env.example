CACHE = true              # Caching does not cache any response related to anime info or source urls, it only caches the key to decrypt 
CACHE_DRIVER = "memory"   # Available options: memory
CACHE_LIFETIME = 1800000  # How long the cache lives on memory before recaching. (milliseconds)